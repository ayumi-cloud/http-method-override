{
    "name": "rstgroup/http-method-override",
    "description": "HTTP method override service",
    "type": "library",
    "keywords": [
        "middleware",
        "psr",
        "psr-7",
        "http",
        "http method overrive",
        "module"
    ],
    "require": {
        "php": "^5.5 || ^7.0"
    },
    "require-dev": {
        "psr/http-message": "^1.0",
        "phpunit/phpunit": "^4.8.21",
        "zendframework/zend-http": "^2.4",
        "zendframework/zend-eventmanager": "^2.4",
        "zendframework/zend-mvc": "^2.4",
        "zendframework/zend-diactoros": "^1.3.3",
        "zendframework/zend-modulemanager": "^2.4"
    },
    "suggest": {
        "zendframework/zend-http": "To use rstgroup/http-method-override as ZF2 module",
        "zendframework/zend-eventmanager": "To use rstgroup/http-method-override as ZF2 module",
        "zendframework/zend-mvc": "To use rstgroup/http-method-override as ZF2 module",
        "zendframework/zend-modulemanager": "To use rstgroup/http-method-override as ZF2 module",
        "psr/http-message": "To use rstgroup/http-method-override as middleware"
    },
    "autoload": {
        "psr-4": {
            "RstGroup\\HttpMethodOverride\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RstGroup\\HttpMethodOverride\\Test\\": "test/"
        }
    }
}
